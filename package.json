{
  "name": "react-webpack-uikit-boilerplate",
  "version": "0.0.1",
  "description": "React Webpack UIKit SCSS Heroku-ready boilerplate app with a test index page",
  "engines": {
    "node": "4.3.1",
    "npm": "3.8.1"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/sebabouche/webpack-react-uikit.git"
  },
  "keywords": [
    "react",
    "webpack",
    "uikit",
    "express",
    "node",
    "heroku",
    "mocha",
    "chai"
  ],
  "author": "Sébastien Nicolaïdis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sebabouche/webpack-react-uikit/issues"
  },
  "homepage": "https://github.com/sebabouche/webpack-react-uikit",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js \"test/**/*.spec.js\"",
    "test:watch": "npm run test -- --watch",
    "start": "node server",
    "build": "webpack --config ./webpack.prod.config.js --progress --colors",
    "clean": "rm -rf dist/*"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.7.7",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "chai": "^3.5.0",
    "chai-immutable": "^1.5.4",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.16.0",
    "ignore-styles": "^1.2.0",
    "image-webpack-loader": "^1.8.0",
    "jsdom": "^8.4.0",
    "mocha": "^2.4.5",
    "node-sass": "^3.5.3",
    "postcss-loader": "^0.8.2",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "classnames": "^2.2.3",
    "css-modules-require-hook": "^4.0.0",
    "express": "^4.13.4",
    "immutable": "^3.8.1",
    "jquery": "^2.2.3",
    "md5": "^2.1.0",
    "react": "^15.0.1",
    "react-ab": "^0.4.5",
    "react-addons-test-utils": "^15.0.2",
    "react-dom": "^15.0.1",
    "uikit": "^2.26.2"
  }
}
